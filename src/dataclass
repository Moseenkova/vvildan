from dataclasses import dataclass, field
@dataclass(order=True)
class FootballPlayer:
    name:str
    surname:str
    value:int
    def __repr__(self):
        return f"FootballPlayer(name='{self.name}', surname='{self.surname}')"

@dataclass(order=True)
class FootballTeam:
    name1:str
    players:list=field(default_factory=list)
    def add_players(self, *args):
        self.players.extend(args)
    def __repr__(self):
        return f"FootballTeam(name1='{self.name1}')"
